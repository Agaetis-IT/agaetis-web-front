{
  "name": "agaetis-web-front",
  "version": "1.0.0",
  "author": "contact@agaetis.fr",
  "license": "GPL-3.0+",
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "@zeit/next-css": "^1.0.1",
    "autoprefixer": "^9.5.1",
    "clsx": "^1.0.4",
    "i18next": "^15.1.0",
    "next": "^8.1.0",
    "next-images": "^1.1.1",
    "next-purgecss": "^3.0.1",
    "normalize.css": "^8.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.9.0",
    "tailwindcss": "^0.7.4"
  },
  "scripts": {
    "test": "jest",
    "dev": "next ./src",
    "build": "next build ./src",
    "start": "next start ./src",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "precommit": "lint-staged && npm test",
    "lint": "tslint -p \"src/**/*.ts?(x)\"",
    "lint:fix": "tslint \"src/**/*.ts?(x)\" --fix",
    "prettier": "prettier \"src/**/*\"",
    "prettier:fix": "prettier \"src/**/*\" --write"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.js?(x)": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/next": "^8.0.5",
    "@types/react": "^16.8.15",
    "@types/react-dom": "^16.8.4",
    "@zeit/next-typescript": "^1.1.1",
    "husky": "^2.1.0",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5"
  }
}
