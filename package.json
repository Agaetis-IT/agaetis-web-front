{
  "name": "blogAgaetis",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "autoprefixer": "^9.5.1",
    "next": "^9.3.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "tailwindcss": "^0.7.4"
  },
  "scripts": {
    "test": "jest",
    "dev": "next ./src",
    "build": "next build ./src",
    "start": "next start ./src",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "precommit": "lint-staged && npm test",
    "lint": "tslint -p \"src/**/*.ts?(x)\"",
    "lint:fix": "tslint \"src/**/*.ts?(x)\" --fix",
    "prettier": "prettier \"src/**/*\"",
    "prettier:fix": "prettier \"src/**/*\" --write"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.js?(x)": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/next": "^8.0.3",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@zeit/next-typescript": "^1.1.1",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.3"
  }
}
